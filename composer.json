{
    "name": "lean-book-tools/phpunit-cleaner-result-printer",
    "description": "PHPUnit result printer for books - simple and short",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "phpunit/phpunit": "^11.0"
    },
    "require-dev": {
        "tracy/tracy": "^2.10",
        "rector/rector": "^1.2",
        "symplify/easy-coding-standard": "^12.1",
        "symfony/process": "^7.0",
        "symfony/finder": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LeanBookTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LeanBookTools\\Test\\": "tests/"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "phpstan": "vendor/bin/phpstan"
    }
}
